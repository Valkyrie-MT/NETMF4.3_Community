    <ProjectView>ShowAllFiles</ProjectView>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)' == 'Debug|AnyCPU'">
    <DeployTransport>USB</DeployTransport>
    <MF_GenerateStubs>True</MF_GenerateStubs>
